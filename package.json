{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap": "^5.2.1",
    "diff2html": "^3.4.19",
    "htmldiff-js": "^1.0.5",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^9.5.1",
    "vue-template-compiler": "^2.7.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {
      "vue/multi-word-component-names": "off",
      "no-underscore-dangle": "off",
      "comma-dangle": "off",
      "no-alert": "off",
      "linebreak-style": "off"
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "eslintIgnore": [
    "src/backend/static"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "vue": {
    "outputDir": "src/backend/static",
    "pages": {
      "index": {
        "entry": "src/frontend/main.js"
      }
    }
  }
}
